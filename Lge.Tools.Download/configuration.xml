<?xml version="1.0" encoding="euc-kr"?>
<!-- encoding must be euc-kr or CP51949 for hangul (save as no bom euc-kr or CP51949 text file) -->
<configuration>
  <header>
    <name>downloader</name>
    <version>1.0.1</version>
    <date>2016-06-08</date>
    <description>downloader configuration for GEN 11</description>
  </header>
  <downloader>
    <!-- logelvel 0:none, 1:error, 2:info, 3:verbose -->
    <loglevel>2</loglevel>
    <protocols>
      <protocol>sahara</protocol>
      <protocol>firehose</protocol>
    </protocols>
    <!-- sahara protocol definition-->
    <sahara>
      <version>
        <min>2</min>
        <max>2</max>
      </version>
      <description>protocol will be used to download device programer</description>
      <images>
        <image>
          <name>PROGRAMER</name>
          <!-- don't change below id number. programer is #13 always. -->
          <id>13</id>
          <path>F:\prj\QPST_DL\ref\_G11_QPST_Downlaoder_자료들\다운로드_관련_유틸리티_툴들\prog_nand_firehose_9x45.mbn</path>
          <description>fire hose protocol program</description>
        </image>
      </images>
    </sahara>
    <!-- fire hose protocol definition-->
    <!-- boolean value is 0:False, 1:True -->
    <firehose>
      <description>protocol will be used to download all partition images</description>
      <configure>
        <memoryName>emmc</memoryName>
        <skipStorageInit>0</skipStorageInit>
        <targetName>9x45</targetName>
        <maxPayloadSizeToTargetInBytes>8192</maxPayloadSizeToTargetInBytes>
        <verbose>0</verbose>
        <allErase>1</allErase>
        <!-- 0: no reset, 1 ~ : reset after #N seconds  -->
        <reset>3</reset>
        <dir></dir>
      </configure>
      <images>
        <!--  firehose's image id must be unique ! -->
        <!--  don't change name 'PARTITION' ! (essential image) -->
        <image name="SBL1" id="1" filename="sbl1.mbn" use="1" erase="0" dump="1">
          <program PAGES_PER_BLOCK="64" SECTOR_SIZE_IN_BYTES="2048" filename="sbl1.mbn" num_partition_sectors="576" physical_partition_number="0" start_sector="0"/>
          <description>Second boot loader image</description>
        </image>
        <image name="PARTITION" id="2" filename="partition_complete_p2K_b128K.mbn" use="1" erase="0" dump="1">
